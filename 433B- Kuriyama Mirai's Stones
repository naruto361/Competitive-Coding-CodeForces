#include <bits/stdc++.h>
using namespace std;
#define ll long long int
#define mod 1000000007
int main()
{
    int n;cin>>n;
    vector<ll> v1,v2;
    v1.push_back(0);
    v2.push_back(0);
    while(n--)
    {
        ll x;cin>>x;
        v1.push_back(x);
        v2.push_back(x);
    }
    sort(v2.begin(),v2.end());
    for(int i=1;i<v1.size();i++)
    {
        v1[i]+=v1[i-1];
        v2[i]+=v2[i-1];
    }
    int q;cin>>q;
    while(q--)
    {
        int t,l,r;cin>>t>>l>>r;
        if(t==1)
        {
            cout<<v1[r]-v1[l-1];
        }
        else cout<<v2[r]-v2[l-1];
        cout<<endl;
    }
}

#include <bits/stdc++.h>
using namespace std;
#define ll long long int
bool isprime(int n) 
{ 
    if (n <= 1)  return false; 
    if (n <= 3)  return true; 
    if (n%2 == 0 || n%3 == 0) return false; 
    for (int i=5; i*i<=n; i=i+6) 
        if (n%i == 0 || n%(i+2) == 0) 
           return false; 
    return true; 
}
void solvep(int n)
{
    int a[n][n]={{0}};
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            if(i==j || i+j==n-1) a[i][j]=1;
            else a[i][j]=0;
        }
    }
    if(n&1)
    {
        a[n/2-1][n/2]=1;
        a[n/2][n/2+1]=1;
    }
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            cout<<a[i][j]<<" ";
        }
        cout<<"\n";
    }
}
int main()
{
    int t;cin>>t;
    while(t--)
    {
        int n;cin>>n;
        solvep(n);
        
    }
    return 0;
}
